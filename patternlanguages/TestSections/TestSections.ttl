@prefix : <http://purl.org/patternpedia/patternlanguages/TestSections#> .
@base <http://purl.org/patternpedia/patternlanguages/TestSections> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://purl.org/patternpedia/patternlanguages/TestSections> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### http://purl.org/patternpedia/patternlanguages/TestSections#hasIcon
<http://purl.org/patternpedia/patternlanguages/TestSections#hasIcon> rdf:type owl:DatatypeProperty .


### http://purl.org/patternpedia/patternlanguages/TestSections#hasContext
<http://purl.org/patternpedia/patternlanguages/TestSections#hasContext> rdf:type owl:DatatypeProperty .


### http://purl.org/patternpedia/patternlanguages/TestSections#hasScore
<http://purl.org/patternpedia/patternlanguages/TestSections#hasScore> rdf:type owl:DatatypeProperty .


### http://purl.org/patternpedia/patternlanguages/TestSections#hasPublicationDate
<http://purl.org/patternpedia/patternlanguages/TestSections#hasPublicationDate> rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/TestSections
:TestSectionsIndividual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestSections#hasIcon> ; 
			 owl:allValuesFrom <http://purl.org/dc/dcmitype/Image>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestSections#hasContext> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestSections#hasScore> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#integer>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestSections#hasPublicationDate> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#date>
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/TestSections
:TestSections rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/204"^^xsd:anyURI ;
pp:hasName "TestSections"^^xsd:string .
:TestSections pp:containsPattern <http://purl.org/patternpedia/patternlanguages/TestSections/Pattern#Pattern> .
#################################################################
# Pattern Import Statements
##############################################################
<http://purl.org/patternpedia/patternlanguages/TestSections> owl:imports <http://purl.org/patternpedia/patternlanguages/TestSections/Pattern> .