@prefix : <http://purl.org/patternpedia/patternlanguages/TokenTest2#> .
@base <http://purl.org/patternpedia/patternlanguages/TokenTest2> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .


<http://purl.org/patternpedia/patternlanguages/TokenTest2> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### Driving Question
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/TokenTest2
:TokenTest2Individual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <Icon> ; 
			 owl:allValuesFrom <xsd:string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <Context> ; 
			 owl:allValuesFrom <xsd:string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <Driving Question> ; 
			 owl:allValuesFrom <xsd:string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <Solution> ; 
			 owl:allValuesFrom <xsd:string>
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/TokenTest2
:TokenTest2 rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "http://placekitten.com/200/312"^^xsd:anyURI ;
pp:hasName "TokenTest2"^^xsd:string .
