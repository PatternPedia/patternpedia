@prefix : <http://purl.org/patternpedia/patternlanguages/TestlanguagePrefixes#> .
@base <http://purl.org/patternpedia/patternlanguages/TestlanguagePrefixes> .


<http://purl.org/patternpedia/patternlanguages/TestlanguagePrefixes> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### Driving Question
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/TestlanguagePrefixes
:TestlanguagePrefixesIndividual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <Icon> ; 
			 owl:allValuesFrom <xsd:string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <Context> ; 
			 owl:allValuesFrom <xsd:string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <Driving Question> ; 
			 owl:allValuesFrom <xsd:string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <Solution> ; 
			 owl:allValuesFrom <xsd:string>
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/TestlanguagePrefixes
:TestlanguagePrefixes rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/200"^^xsd:anyURI ;
pp:hasName "TestlanguagePrefixes"^^xsd:string .
