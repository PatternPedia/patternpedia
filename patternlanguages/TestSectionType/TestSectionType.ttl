@prefix : <http://purl.org/patternpedia/patternlanguages/TestSectionType#> .
@base <http://purl.org/patternpedia/patternlanguages/TestSectionType> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://purl.org/patternpedia/patternlanguages/TestSectionType> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### DrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


### Score
:hasScore rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/TestSectionType#TestSectionType
:TestSectionTypeIndividual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestSectionType#hasIcon> ; 
			 owl:allValuesFrom <http://purl.org/dc/dcmitype/Image>
				
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestSectionType#hasContext> ; 
				      owl:QualifiedCardinality"1"^^xsd:nonNegativeInteger;
			 owl:onDataRange xsd:string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestSectionType#hasDrivingQuestion> ; 
			 owl:maxCardinality "2"^^xsd:nonNegativeInteger;
			 owl:onDataRange <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestSectionType#hasSolution> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestSectionType#hasScore> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#integer>
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/TestSectionType#TestSectionType
:TestSectionType rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/200"^^xsd:anyURI ;
pp:hasName "TestSectionType"^^xsd:string .
:TestSectionType pp:containsPattern <http://purl.org/patternpedia/patternlanguages/TestSectionType/Pattern2#Pattern2> .
:TestSectionType pp:containsPattern <http://purl.org/patternpedia/patternlanguages/TestSectionType/Patternname#Patternname> .
:TestSectionType pp:containsPattern <http://purl.org/patternpedia/patternlanguages/TestSectionType/Pattern3#Pattern3> .
#################################################################
# Pattern Import Statements
##############################################################
<http://purl.org/patternpedia/patternlanguages/TestSectionType> owl:imports <http://purl.org/patternpedia/patternlanguages/TestSectionType/Pattern2> .
<http://purl.org/patternpedia/patternlanguages/TestSectionType> owl:imports <http://purl.org/patternpedia/patternlanguages/TestSectionType/Patternname> .
<http://purl.org/patternpedia/patternlanguages/TestSectionType> owl:imports <http://purl.org/patternpedia/patternlanguages/TestSectionType/Pattern3> .
