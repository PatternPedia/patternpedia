@prefix : <http://purl.org/patternpedia/patternlanguages/TestR#> .
@base <http://purl.org/patternpedia/patternlanguages/TestR> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://purl.org/patternpedia/patternlanguages/TestR> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### DrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


### SolutionSketches
:hasSolutionSketches rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/TestR#TestR
:TestRIndividual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestR#hasIcon> ; 
			 owl:allValuesFrom http://www.w3.org/2001/XMLSchema#string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestR#hasContext> ; 
			 owl:someValuesFrom http://www.w3.org/2001/XMLSchema#string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestR#hasDrivingQuestion> ; 
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestR#hasSolution> ; 
			 owl:minCardinality "1"^^xsd:nonNegativeInteger;
			 owl:onDataRange http://www.w3.org/2001/XMLSchema#string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestR#hasSolutionSketches> ; 
			 owl:maxCardinality "1"^^xsd:nonNegativeInteger;
			 owl:onDataRange http://www.w3.org/2001/XMLSchema#positiveInteger
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/TestR#TestR
:TestR rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/200"^^xsd:anyURI ;
pp:hasName "TestR"^^xsd:string .
:TestR pp:containsPattern <http://purl.org/patternpedia/patternlanguages/TestR/Pat#Pat> .
#################################################################
# Pattern Import Statements
##############################################################
<http://purl.org/patternpedia/patternlanguages/TestR> owl:imports <http://purl.org/patternpedia/patternlanguages/TestR/Pat> .