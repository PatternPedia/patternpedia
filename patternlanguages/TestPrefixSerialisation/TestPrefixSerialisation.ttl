@prefix : <http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation#> .
@base <http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### DrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation#TestPrefixSerialisation
:TestPrefixSerialisationIndividual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation#hasIcon> ; 
			 owl:allValuesFrom <https://placekitten.com/200/430test>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation#hasContext> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation#hasDrivingQuestion> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation#hasSolution> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation#TestPrefixSerialisation
:TestPrefixSerialisation rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/233"^^xsd:anyURI ;
pp:hasName "TestPrefixSerialisation"^^xsd:string .
:TestPrefixSerialisation pp:containsPattern <http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation/Pat#Pat> .
#################################################################
# Pattern Import Statements
##############################################################
<http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation> owl:imports <http://purl.org/patternpedia/patternlanguages/TestPrefixSerialisation/Pat> .