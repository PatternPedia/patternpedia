@prefix : <http://purl.org/patternpedia/patternlanguages/Test34#> .
@base <http://purl.org/patternpedia/patternlanguages/Test34> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://purl.org/patternpedia/patternlanguages/Test34> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### DrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/Test34#Test34
:Test34Individual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/Test34#hasIcon> ; 
			 owl:allValuesFrom http://www.w3.org/2001/XMLSchema#string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/Test34#hasContext> ; 
			 owl:maxCardinality "1"^^xsd:nonNegativeInteger;
			 owl:onDataRange <xsd:anyURI>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/Test34#hasDrivingQuestion> ; 
			 owl:someValuesFrom http://www.w3.org/2001/XMLSchema#string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/Test34#hasSolution> ; 
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/Test34#Test34
:Test34 rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/200"^^xsd:anyURI ;
pp:hasName "Test34"^^xsd:string .
:Test34 pp:containsPattern <http://purl.org/patternpedia/patternlanguages/Test34/Pattern1#Pattern1> .
#################################################################
# Pattern Import Statements
##############################################################
<http://purl.org/patternpedia/patternlanguages/Test34> owl:imports <http://purl.org/patternpedia/patternlanguages/Test34/Pattern1> .