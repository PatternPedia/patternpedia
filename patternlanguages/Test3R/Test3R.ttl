@prefix : <http://purl.org/patternpedia/patternlanguages/Test3R#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/patternpedia/patternlanguages/Test3R> .


<http://purl.org/patternpedia/patternlanguages/Test3R> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### Driving Question
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/Test3R
:Test3RIndividual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasIcon ; 
			 owl:allValuesFrom xsd:string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasContext ; 
			 owl:someValuesFrom xsd:string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasDrivingQuestion ; 
			 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasSolution ; 
			 owl:maxQualifiedCardinality "5"^^xsd:nonNegativeInteger
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasSolution ; 
			 owl:QualifiedCardinality"3"^^xsd:nonNegativeInteger
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/Test3R
:Test3R rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/204"^^xsd:anyURI ;
pp:hasName "Test3R"^^xsd:string .
