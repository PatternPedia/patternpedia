@prefix : <http://purl.org/patternpedia/patternlanguages/p#> .
@base <http://purl.org/patternpedia/patternlanguages/p> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://purl.org/patternpedia/patternlanguages/p> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### DrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/p#p
:pIndividual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/p#hasIcon> ; 
			 owl:allValuesFrom http://www.w3.org/2001/XMLSchema#string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/p#hasContext> ; 
			 owl:someValuesFrom http://www.w3.org/2001/XMLSchema#string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/p#hasDrivingQuestion> ; 
			 owl:minCardinality "0"^^xsd:nonNegativeInteger;
			 owl:onDataRange http://www.w3.org/2001/XMLSchema#string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/p#hasSolution> ; 
			 owl:allValuesFrom http://www.w3.org/2001/XMLSchema#string
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/p#p
:p rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/200"^^xsd:anyURI ;
pp:hasName "p"^^xsd:string .
:p pp:containsPattern <http://purl.org/patternpedia/patternlanguages/p/Patter#Patter> .
#################################################################
# Pattern Import Statements
##############################################################
<http://purl.org/patternpedia/patternlanguages/p> owl:imports <http://purl.org/patternpedia/patternlanguages/p/Patter> .