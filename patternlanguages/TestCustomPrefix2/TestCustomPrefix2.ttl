@prefix : <http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2#> .
@base <http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### DrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2#TestCustomPrefix2
:TestCustomPrefix2Individual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2#hasIcon> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2#hasContext> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2#hasDrivingQuestion> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2#hasSolution> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2#TestCustomPrefix2
:TestCustomPrefix2 rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "http://placekitten.com/150/153"^^xsd:anyURI ;
pp:hasName "TestCustomPrefix2"^^xsd:string .
:TestCustomPrefix2 pp:containsPattern <http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2/Patternname#Patternname> .
#################################################################
# Pattern Import Statements
##############################################################
<http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2> owl:imports <http://purl.org/patternpedia/patternlanguages/TestCustomPrefix2/Patternname> .