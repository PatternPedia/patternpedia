@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<two-tiercloudapplication-to-hybriddata>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/two-tiercloudapplication> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hybriddata> . 


#################################################################
#    Individual
#################################################################

<two-tiercloudapplication-to-hybriddata#Two-TierCloudApplication-to-HybridData>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <two-tiercloudapplication#Two-TierCloudApplication> ; 
    pp:hasTarget <hybriddata#HybridData> . 

