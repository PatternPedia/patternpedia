@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<environment-basedavailability-to-resiliencymanagementprocess>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/environment-basedavailability> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/resiliencymanagementprocess> . 


#################################################################
#    Individual
#################################################################

<environment-basedavailability-to-resiliencymanagementprocess#Environment-basedAvailability-to-ResiliencyManagementProcess>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <environment-basedavailability#Environment-basedAvailability> ; 
    pp:hasTarget <resiliencymanagementprocess#ResiliencyManagementProcess> . 

