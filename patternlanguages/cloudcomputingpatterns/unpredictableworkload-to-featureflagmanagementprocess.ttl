@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<unpredictableworkload-to-featureflagmanagementprocess>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/unpredictableworkload> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/featureflagmanagementprocess> . 


#################################################################
#    Individual
#################################################################

<unpredictableworkload-to-featureflagmanagementprocess#UnpredictableWorkload-to-FeatureFlagManagementProcess>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <unpredictableworkload#UnpredictableWorkload> ; 
    pp:hasTarget <featureflagmanagementprocess#FeatureFlagManagementProcess> . 

