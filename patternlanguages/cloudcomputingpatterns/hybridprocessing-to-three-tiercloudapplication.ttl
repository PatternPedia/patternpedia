@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<hybridprocessing-to-three-tiercloudapplication>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hybridprocessing> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/three-tiercloudapplication> . 


#################################################################
#    Individual
#################################################################

<hybridprocessing-to-three-tiercloudapplication#HybridProcessing-to-Three-TierCloudApplication>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <hybridprocessing#HybridProcessing> ; 
    pp:hasTarget <three-tiercloudapplication#Three-TierCloudApplication> . 

