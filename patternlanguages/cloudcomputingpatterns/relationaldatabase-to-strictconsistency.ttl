@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<relationaldatabase-to-strictconsistency>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/relationaldatabase> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/strictconsistency> . 


#################################################################
#    Individual
#################################################################

<relationaldatabase-to-strictconsistency#RelationalDatabase-to-StrictConsistency>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <relationaldatabase#RelationalDatabase> ; 
    pp:hasTarget <strictconsistency#StrictConsistency> . 

