@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<statelesscomponent-to-message-orientedmiddleware>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/statelesscomponent> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/message-orientedmiddleware> . 


#################################################################
#    Individual
#################################################################

<statelesscomponent-to-message-orientedmiddleware#StatelessComponent-to-Message-orientedMiddleware>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <statelesscomponent#StatelessComponent> ; 
    pp:hasTarget <message-orientedmiddleware#Message-orientedMiddleware> . 

