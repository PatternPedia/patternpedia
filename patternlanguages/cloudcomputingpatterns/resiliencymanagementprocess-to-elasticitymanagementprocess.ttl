@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<resiliencymanagementprocess-to-elasticitymanagementprocess>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/resiliencymanagementprocess> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/elasticitymanagementprocess> . 


#################################################################
#    Individual
#################################################################

<resiliencymanagementprocess-to-elasticitymanagementprocess#ResiliencyManagementProcess-to-ElasticityManagementProcess>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <resiliencymanagementprocess#ResiliencyManagementProcess> ; 
    pp:hasTarget <elasticitymanagementprocess#ElasticityManagementProcess> . 

