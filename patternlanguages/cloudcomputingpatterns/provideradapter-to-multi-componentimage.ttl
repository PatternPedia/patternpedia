@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<provideradapter-to-multi-componentimage>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/provideradapter> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/multi-componentimage> . 


#################################################################
#    Individual
#################################################################

<provideradapter-to-multi-componentimage#ProviderAdapter-to-Multi-ComponentImage>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <provideradapter#ProviderAdapter> ; 
    pp:hasTarget <multi-componentimage#Multi-ComponentImage> . 

