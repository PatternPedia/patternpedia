@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<integrationprovider-to-applicationcomponentproxy>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/integrationprovider> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/applicationcomponentproxy> . 


#################################################################
#    Individual
#################################################################

<integrationprovider-to-applicationcomponentproxy#IntegrationProvider-to-ApplicationComponentProxy>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <integrationprovider#IntegrationProvider> ; 
    pp:hasTarget <applicationcomponentproxy#ApplicationComponentProxy> . 

