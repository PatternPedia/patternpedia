@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<statefulcomponent-to-eventualconsistency>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/statefulcomponent> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/eventualconsistency> . 


#################################################################
#    Individual
#################################################################

<statefulcomponent-to-eventualconsistency#StatefulComponent-to-EventualConsistency>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <statefulcomponent#StatefulComponent> ; 
    pp:hasTarget <eventualconsistency#EventualConsistency> . 

