@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<dedicatedcomponent>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> .


#################################################################
#    Individual
#################################################################

<dedicatedcomponent#DedicatedComponent>
    rdf:type owl:NamedIndividual , <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#CloudComputingPattern> ;
    <http://purl.org/patternpedia#hasName> "Dedicated Component"^^xsd:string ;
    :hasIcon "http://www.cloudcomputingpatterns.org/icons/dedicated_component_icon.png"^^xsd:anyURI ;
    :hasContext "A Distributed Application is offered to multiple tenants. These tenants share IT resources required by applications provided to them."^^xsd:string ;
    :hasDrivingQuestion "How can application components that cannot be shared be integrated into a multi-tenant application?"^^xsd:string ;
    :hasIntent "Components providing critical functionality shall be provided exclusively to tenants while still allowing other components to be shared between tenants."^^xsd:string ;
    :hasSolution "Dedicated application components are provided exclusively for each tenant using the application."^^xsd:string ;
    :hasSolutionSketch "http://www.cloudcomputingpatterns.org/sketches/dedicated_component_sketch.png"^^xsd:anyURI .

