@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<dataaccesscomponent-to-compliantdatareplication>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/dataaccesscomponent> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/compliantdatareplication> . 


#################################################################
#    Individual
#################################################################

<dataaccesscomponent-to-compliantdatareplication#DataAccessComponent-to-CompliantDataReplication>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <dataaccesscomponent#DataAccessComponent> ; 
    pp:hasTarget <compliantdatareplication#CompliantDataReplication> . 

