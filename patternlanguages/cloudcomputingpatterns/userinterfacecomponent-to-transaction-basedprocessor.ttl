@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<userinterfacecomponent-to-transaction-basedprocessor>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/userinterfacecomponent> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/transaction-basedprocessor> . 


#################################################################
#    Individual
#################################################################

<userinterfacecomponent-to-transaction-basedprocessor#UserInterfaceComponent-to-Transaction-basedProcessor>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <userinterfacecomponent#UserInterfaceComponent> ; 
    pp:hasTarget <transaction-basedprocessor#Transaction-basedProcessor> . 

