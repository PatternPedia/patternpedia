@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<virtualnetworking-to-hypervisor>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/virtualnetworking> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hypervisor> . 


#################################################################
#    Individual
#################################################################

<virtualnetworking-to-hypervisor#VirtualNetworking-to-Hypervisor>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <virtualnetworking#VirtualNetworking> ; 
    pp:hasTarget <hypervisor#Hypervisor> . 

