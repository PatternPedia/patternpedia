@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<watchdog-to-elasticitymanager>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/watchdog> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/elasticitymanager> . 


#################################################################
#    Individual
#################################################################

<watchdog-to-elasticitymanager#Watchdog-to-ElasticityManager>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <watchdog#Watchdog> ; 
    pp:hasTarget <elasticitymanager#ElasticityManager> . 

