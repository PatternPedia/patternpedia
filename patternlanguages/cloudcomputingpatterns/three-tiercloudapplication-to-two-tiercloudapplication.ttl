@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<three-tiercloudapplication-to-two-tiercloudapplication>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/three-tiercloudapplication> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/two-tiercloudapplication> . 


#################################################################
#    Individual
#################################################################

<three-tiercloudapplication-to-two-tiercloudapplication#Three-TierCloudApplication-to-Two-TierCloudApplication>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <three-tiercloudapplication#Three-TierCloudApplication> ; 
    pp:hasTarget <two-tiercloudapplication#Two-TierCloudApplication> . 

