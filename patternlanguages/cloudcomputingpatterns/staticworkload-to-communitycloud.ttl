@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<staticworkload-to-communitycloud>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/staticworkload> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/communitycloud> . 


#################################################################
#    Individual
#################################################################

<staticworkload-to-communitycloud#StaticWorkload-to-CommunityCloud>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <staticworkload#StaticWorkload> ; 
    pp:hasTarget <communitycloud#CommunityCloud> . 

