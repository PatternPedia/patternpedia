@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<integrationprovider-to-hybriddata>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/integrationprovider> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hybriddata> . 


#################################################################
#    Individual
#################################################################

<integrationprovider-to-hybriddata#IntegrationProvider-to-HybridData>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <integrationprovider#IntegrationProvider> ; 
    pp:hasTarget <hybriddata#HybridData> . 

