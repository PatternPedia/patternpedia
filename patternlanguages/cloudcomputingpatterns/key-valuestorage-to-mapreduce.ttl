@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<key-valuestorage-to-mapreduce>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/key-valuestorage> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/mapreduce> . 


#################################################################
#    Individual
#################################################################

<key-valuestorage-to-mapreduce#Key-ValueStorage-to-MapReduce>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <key-valuestorage#Key-ValueStorage> ; 
    pp:hasTarget <mapreduce#MapReduce> . 

