@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<infrastructureasaservice-to-virtualnetworking>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/infrastructureasaservice> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/virtualnetworking> . 


#################################################################
#    Individual
#################################################################

<infrastructureasaservice-to-virtualnetworking#InfrastructureasaService-to-VirtualNetworking>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <infrastructureasaservice#InfrastructureasaService> ; 
    pp:hasTarget <virtualnetworking#VirtualNetworking> . 

