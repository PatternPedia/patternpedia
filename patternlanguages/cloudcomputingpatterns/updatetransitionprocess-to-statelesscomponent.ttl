@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<updatetransitionprocess-to-statelesscomponent>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/updatetransitionprocess> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/statelesscomponent> . 


#################################################################
#    Individual
#################################################################

<updatetransitionprocess-to-statelesscomponent#UpdateTransitionProcess-to-StatelessComponent>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <updatetransitionprocess#UpdateTransitionProcess> ; 
    pp:hasTarget <statelesscomponent#StatelessComponent> . 

