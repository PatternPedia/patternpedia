@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<hybridmultimediawebapplication-to-hybridapplicationfunctions>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hybridmultimediawebapplication> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hybridapplicationfunctions> . 


#################################################################
#    Individual
#################################################################

<hybridmultimediawebapplication-to-hybridapplicationfunctions#HybridMultimediaWebApplication-to-HybridApplicationFunctions>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <hybridmultimediawebapplication#HybridMultimediaWebApplication> ; 
    pp:hasTarget <hybridapplicationfunctions#HybridApplicationFunctions> . 

