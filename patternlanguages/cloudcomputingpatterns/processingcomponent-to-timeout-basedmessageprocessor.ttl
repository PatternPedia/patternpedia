@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<processingcomponent-to-timeout-basedmessageprocessor>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/processingcomponent> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/timeout-basedmessageprocessor> . 


#################################################################
#    Individual
#################################################################

<processingcomponent-to-timeout-basedmessageprocessor#ProcessingComponent-to-Timeout-basedMessageProcessor>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <processingcomponent#ProcessingComponent> ; 
    pp:hasTarget <timeout-basedmessageprocessor#Timeout-basedMessageProcessor> . 

