@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<hybriddata-to-contentdistributionnetwork>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hybriddata> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/contentdistributionnetwork> . 


#################################################################
#    Individual
#################################################################

<hybriddata-to-contentdistributionnetwork#HybridData-to-ContentDistributionNetwork>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <hybriddata#HybridData> ; 
    pp:hasTarget <contentdistributionnetwork#ContentDistributionNetwork> . 

