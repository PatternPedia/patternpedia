@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<platformasaservice-to-elasticplatform>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/platformasaservice> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/elasticplatform> . 


#################################################################
#    Individual
#################################################################

<platformasaservice-to-elasticplatform#PlatformasaService-to-ElasticPlatform>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <platformasaservice#PlatformasaService> ; 
    pp:hasTarget <elasticplatform#ElasticPlatform> . 

