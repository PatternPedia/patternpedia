@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<hybridmultimediawebapplication-to-three-tiercloudapplication>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hybridmultimediawebapplication> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/three-tiercloudapplication> . 


#################################################################
#    Individual
#################################################################

<hybridmultimediawebapplication-to-three-tiercloudapplication#HybridMultimediaWebApplication-to-Three-TierCloudApplication>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <hybridmultimediawebapplication#HybridMultimediaWebApplication> ; 
    pp:hasTarget <three-tiercloudapplication#Three-TierCloudApplication> . 

