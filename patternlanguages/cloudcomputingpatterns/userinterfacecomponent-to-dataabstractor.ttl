@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<userinterfacecomponent-to-dataabstractor>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/userinterfacecomponent> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/dataabstractor> . 


#################################################################
#    Individual
#################################################################

<userinterfacecomponent-to-dataabstractor#UserInterfaceComponent-to-DataAbstractor>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <userinterfacecomponent#UserInterfaceComponent> ; 
    pp:hasTarget <dataabstractor#DataAbstractor> . 

