@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<batchprocessingcomponent-to-timeout-basedmessageprocessor>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/batchprocessingcomponent> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/timeout-basedmessageprocessor> . 


#################################################################
#    Individual
#################################################################

<batchprocessingcomponent-to-timeout-basedmessageprocessor#BatchProcessingComponent-to-Timeout-basedMessageProcessor>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <batchprocessingcomponent#BatchProcessingComponent> ; 
    pp:hasTarget <timeout-basedmessageprocessor#Timeout-basedMessageProcessor> . 

