@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<hybridcloud-to-publiccloud>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hybridcloud> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/publiccloud> . 


#################################################################
#    Individual
#################################################################

<hybridcloud-to-publiccloud#HybridCloud-to-PublicCloud>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <hybridcloud#HybridCloud> ; 
    pp:hasTarget <publiccloud#PublicCloud> . 

