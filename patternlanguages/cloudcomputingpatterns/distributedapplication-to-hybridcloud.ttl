@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<distributedapplication-to-hybridcloud>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/distributedapplication> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hybridcloud> . 


#################################################################
#    Individual
#################################################################

<distributedapplication-to-hybridcloud#DistributedApplication-to-HybridCloud>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <distributedapplication#DistributedApplication> ; 
    pp:hasTarget <hybridcloud#HybridCloud> . 

