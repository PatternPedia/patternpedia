@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<node-basedavailability-to-resiliencymanagementprocess>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/node-basedavailability> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/resiliencymanagementprocess> . 


#################################################################
#    Individual
#################################################################

<node-basedavailability-to-resiliencymanagementprocess#Node-basedAvailability-to-ResiliencyManagementProcess>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <node-basedavailability#Node-basedAvailability> ; 
    pp:hasTarget <resiliencymanagementprocess#ResiliencyManagementProcess> . 

