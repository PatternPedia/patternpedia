@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<periodicworkload-to-privatecloud>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/periodicworkload> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/privatecloud> . 


#################################################################
#    Individual
#################################################################

<periodicworkload-to-privatecloud#PeriodicWorkload-to-PrivateCloud>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <periodicworkload#PeriodicWorkload> ; 
    pp:hasTarget <privatecloud#PrivateCloud> . 

