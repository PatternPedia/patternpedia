@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<idempotentprocessor-to-dataaccesscomponent>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/idempotentprocessor> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/dataaccesscomponent> . 


#################################################################
#    Individual
#################################################################

<idempotentprocessor-to-dataaccesscomponent#IdempotentProcessor-to-DataAccessComponent>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <idempotentprocessor#IdempotentProcessor> ; 
    pp:hasTarget <dataaccesscomponent#DataAccessComponent> . 

