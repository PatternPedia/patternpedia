@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<once-in-a-lifetimeworkload-to-publiccloud>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/once-in-a-lifetimeworkload> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/publiccloud> . 


#################################################################
#    Individual
#################################################################

<once-in-a-lifetimeworkload-to-publiccloud#Once-in-a-lifetimeWorkload-to-PublicCloud>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <once-in-a-lifetimeworkload#Once-in-a-lifetimeWorkload> ; 
    pp:hasTarget <publiccloud#PublicCloud> . 

