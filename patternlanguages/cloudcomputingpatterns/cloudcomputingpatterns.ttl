#################################################################
#    The ontology of the patternlanguage
#################################################################

<https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/cloudcomputingpatterns-Patterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/cloudcomputingpatterns-Relations> ,
        # views
        <https://purl.org/patternpedia/patternviews/cloudcomputingpatterns-enterpriseintegrationpatterns-view> ,
        <https://purl.org/patternpedia/patternviews/cloudcomputingpatterns-enterpriseapplicationarchitecturepatterns-view> ,
        <https://purl.org/patternpedia/patternviews/cloudcomputingpatterns-gangoffourpatterns-view> ,
        <https://purl.org/patternpedia/patternviews/cloudcomputingpatterns-internetofthingspatterns-view> ,
        <https://purl.org/patternpedia/patternviews/cloudcomputingpatterns-patternorientedsoftwarearchitecturepatterns-view> .


#################################################################
#    Data Properties
#################################################################

:hasContext rdf:type owl:DatatypeProperty .

:hasDrivingQuestion rdf:type owl:DatatypeProperty .

:hasIcon rdf:type owl:DatatypeProperty .

:hasIntent rdf:type owl:DatatypeProperty .

:hasResult rdf:type owl:DatatypeProperty .

:hasSolution rdf:type owl:DatatypeProperty .

:hasSolutionSketch rdf:type owl:DatatypeProperty .

:hasVariation rdf:type owl:DatatypeProperty .


#################################################################
#    Class definitions
#################################################################

:CloudComputingPattern
    rdf:type owl:Class ;
    rdfs:subClassOf pp:Pattern ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasSolutionSketch ;
          owl:allValuesFrom xsd:anyURI
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasIcon ;
          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onDataRange xsd:anyURI
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasDrivingQuestion ;
          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onDataRange xsd:string
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasContext ;
          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onDataRange xsd:string
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasIntent ;
          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onDataRange xsd:string
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasSolution ;
          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onDataRange xsd:string
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasResult ;
          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          owl:onDataRange xsd:string
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasVariation ;
          owl:allValuesFrom xsd:string
        ] .

