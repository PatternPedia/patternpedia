@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<elasticitymanagementprocess-to-elasticinfrastructure>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/elasticitymanagementprocess> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/elasticinfrastructure> . 


#################################################################
#    Individual
#################################################################

<elasticitymanagementprocess-to-elasticinfrastructure#ElasticityManagementProcess-to-ElasticInfrastructure>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <elasticitymanagementprocess#ElasticityManagementProcess> ; 
    pp:hasTarget <elasticinfrastructure#ElasticInfrastructure> . 

