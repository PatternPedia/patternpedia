@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<elasticitymanagementprocess-to-statelesscomponent>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/elasticitymanagementprocess> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/statelesscomponent> . 


#################################################################
#    Individual
#################################################################

<elasticitymanagementprocess-to-statelesscomponent#ElasticityManagementProcess-to-StatelessComponent>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <elasticitymanagementprocess#ElasticityManagementProcess> ; 
    pp:hasTarget <statelesscomponent#StatelessComponent> . 

