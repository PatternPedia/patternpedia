@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<featureflagmanagementprocess-to-standbypoolingprocess>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/featureflagmanagementprocess> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/standbypoolingprocess> . 


#################################################################
#    Individual
#################################################################

<featureflagmanagementprocess-to-standbypoolingprocess#FeatureFlagManagementProcess-to-StandbyPoolingProcess>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <featureflagmanagementprocess#FeatureFlagManagementProcess> ; 
    pp:hasTarget <standbypoolingprocess#StandbyPoolingProcess> . 

