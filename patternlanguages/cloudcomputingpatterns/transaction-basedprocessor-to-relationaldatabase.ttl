@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<transaction-basedprocessor-to-relationaldatabase>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/transaction-basedprocessor> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/relationaldatabase> . 


#################################################################
#    Individual
#################################################################

<transaction-basedprocessor-to-relationaldatabase#Transaction-basedProcessor-to-RelationalDatabase>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <transaction-basedprocessor#Transaction-basedProcessor> ; 
    pp:hasTarget <relationaldatabase#RelationalDatabase> . 

