@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<message-orientedmiddleware-to-at-least-oncedelivery>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/message-orientedmiddleware> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/at-least-oncedelivery> . 


#################################################################
#    Individual
#################################################################

<message-orientedmiddleware-to-at-least-oncedelivery#Message-orientedMiddleware-to-At-least-onceDelivery>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <message-orientedmiddleware#Message-orientedMiddleware> ; 
    pp:hasTarget <at-least-oncedelivery#At-least-onceDelivery> . 

