@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<blockstorage-to-environment-basedavailability>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/blockstorage> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/environment-basedavailability> . 


#################################################################
#    Individual
#################################################################

<blockstorage-to-environment-basedavailability#BlockStorage-to-Environment-basedAvailability>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <blockstorage#BlockStorage> ; 
    pp:hasTarget <environment-basedavailability#Environment-basedAvailability> . 

