@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<timeout-basedmessageprocessor-to-watchdog>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/timeout-basedmessageprocessor> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/watchdog> . 


#################################################################
#    Individual
#################################################################

<timeout-basedmessageprocessor-to-watchdog#Timeout-basedMessageProcessor-to-Watchdog>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <timeout-basedmessageprocessor#Timeout-basedMessageProcessor> ; 
    pp:hasTarget <watchdog#Watchdog> . 

