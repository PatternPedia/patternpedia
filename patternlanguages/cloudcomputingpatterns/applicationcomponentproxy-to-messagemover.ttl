@prefix : <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<applicationcomponentproxy-to-messagemover>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/applicationcomponentproxy> , 
        <http://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/messagemover> . 


#################################################################
#    Individual
#################################################################

<applicationcomponentproxy-to-messagemover#ApplicationComponentProxy-to-MessageMover>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <applicationcomponentproxy#ApplicationComponentProxy> ; 
    pp:hasTarget <messagemover#MessageMover> . 

