@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<key-valuestorage-to-dataaccesscomponent>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/key-valuestorage> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/dataaccesscomponent> . 


#################################################################
#    Individual
#################################################################

<key-valuestorage-to-dataaccesscomponent#Key-ValueStorage-to-DataAccessComponent>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <key-valuestorage#Key-ValueStorage> ; 
    pp:hasTarget <dataaccesscomponent#DataAccessComponent> . 

