@prefix : <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns#> .
@prefix pp: <https://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/> .

#################################################################
#    Ontology
#################################################################

<elasticinfrastructure-to-hypervisor>
    rdf:type owl:Ontology ;
    owl:imports <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns> ,
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/elasticinfrastructure> , 
        <https://purl.org/patternpedia/patternlanguages/cloudcomputingpatterns/hypervisor> . 


#################################################################
#    Individual
#################################################################

<elasticinfrastructure-to-hypervisor#ElasticInfrastructure-to-Hypervisor>
    rdf:type owl:NamedIndividual , pp:DirectedPatternRelationDescriptor ;
    pp:hasSource <elasticinfrastructure#ElasticInfrastructure> ; 
    pp:hasTarget <hypervisor#Hypervisor> . 

