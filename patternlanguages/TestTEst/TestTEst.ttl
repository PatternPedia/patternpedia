@prefix : <http://purl.org/patternpedia/patternlanguages/TestTEst#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/patternpedia/patternlanguages/TestTEst> .


<http://purl.org/patternpedia/patternlanguages/TestTEst> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### DrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/TestTEst#TestTEst
:TestTEstIndividual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty http://purl.org/patternpedia/patternlanguages/TestTEst#hasIcon ; 
			 owl:allValuesFrom http://www.w3.org/2001/XMLSchema#anyURI
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty http://purl.org/patternpedia/patternlanguages/TestTEst#hasContext ; 
			 owl:someValuesFrom http://www.w3.org/2001/XMLSchema#string
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty http://purl.org/patternpedia/patternlanguages/TestTEst#hasDrivingQuestion ; 
			 owl:allValuesFrom http://www.w3.org/2001/XMLSchema#positiveInteger
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty http://purl.org/patternpedia/patternlanguages/TestTEst#hasSolution ; 
			 owl:allValuesFrom http://www.w3.org/2001/XMLSchema#string
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/TestTEst#TestTEst
:TestTEst rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/200"^^xsd:anyURI ;
pp:hasName "TestTEst"^^xsd:string .
:TestTEst pp:containsPattern <http://purl.org/patternpedia/patternlanguages/TestTEst/Pat#Pat> .
#################################################################
# Pattern Import Statements
##############################################################
<http://purl.org/patternpedia/patternlanguages/TestTEst> owl:imports <http://purl.org/patternpedia/patternlanguages/TestTEst/Pat> .
