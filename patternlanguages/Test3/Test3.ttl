@prefix : <http://purl.org/patternpedia/patternlanguages/Test3#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/patternpedia/patternlanguages/Test3> .


<http://purl.org/patternpedia/patternlanguages/Test3> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### Driving Question
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


### Solution Sketches
:hasSolutionSketches rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/Test3
:Test3Individual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasIcon ; 
			 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ; 
			 owl:onDataRange <xsd:anyURI>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasContext ; 
			 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ; 
			 owl:onDataRange <xsd:string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasDrivingQuestion ; 
			 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ; 
			 owl:onDataRange <xsd:string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasSolution ; 
			 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ; 
			 owl:onDataRange <xsd:string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty :hasSolutionSketches ; 
			 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ; 
			 owl:onDataRange <xsd:anyURI>
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/Test3
:Test3 rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/203"^^xsd:anyURI ;
pp:hasName "Test3"^^xsd:string .
