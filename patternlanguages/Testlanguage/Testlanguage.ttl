@prefix : <http://purl.org/patternpedia/patternlanguages/Testlanguage#> .
@base <http://purl.org/patternpedia/patternlanguages/Testlanguage> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://purl.org/patternpedia/patternlanguages/Testlanguage> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


### Icon
:hasIcon rdf:type owl:DatatypeProperty .


### Context
:hasContext rdf:type owl:DatatypeProperty .


### DrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


### Solution
:hasSolution rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/patternlanguages/Testlanguage#Testlanguage
:TestlanguageIndividual rdf:type owl:Class ; 
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/Testlanguage#hasIcon> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/Testlanguage#hasContext> ; 
			 owl:minCardinality "0"^^xsd:nonNegativeInteger;
			 owl:onDataRange <http://www.w3.org/2001/XMLSchema#string>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/Testlanguage#hasDrivingQuestion> ; 
			 owl:someValuesFrom <http://www.w3.org/2001/XMLSchema#anyURI>
				] ,


			[ rdf:type owl:Restriction ;
			 owl:onProperty <http://purl.org/patternpedia/patternlanguages/Testlanguage#hasSolution> ; 
			 owl:allValuesFrom <http://www.w3.org/2001/XMLSchema#string>
				] .


#################################################################
# Individuals
##############################################################
###  http://purl.org/patternpedia/patternlanguages/Testlanguage#Testlanguage
:Testlanguage rdf:type owl:NamedIndividual ,
pp:PatternLanguage ;
pp:hasLogo "https://placekitten.com/300/200"^^xsd:anyURI ;
pp:hasName "Testlanguage"^^xsd:string .
:Testlanguage pp:containsPattern <http://purl.org/patternpedia/patternlanguages/Testlanguage/Pat#Pat> .
#################################################################
# Pattern Import Statements
##############################################################
<http://purl.org/patternpedia/patternlanguages/Testlanguage> owl:imports <http://purl.org/patternpedia/patternlanguages/Testlanguage/Pat> .